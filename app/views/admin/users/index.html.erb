<% @users.each do |user| %>
  Civilité : <%= user.title %><br/>
  Prénom : <%= user.firstname %><br/>
  Nom : <%= user.lastname %><br/>
  Date de naissance : <%= user.birthday %><br/>
  Email : <%= user.email %><br/>
  Ville : <%= user.city %><br/>
  Revenu mensuel : <%= user.monthly_salary_range.range %><br/>
  <%= link_to "Editer", edit_admin_user_path(user), class: 'btn btn-primary btn-sm' %>
  <%= link_to "Supprimer", admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' },
    class: 'btn btn-danger btn-sm' %>
  <br/><br/>
<% end %>
